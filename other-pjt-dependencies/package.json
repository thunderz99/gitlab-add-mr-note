    "scripts": {
        "prestart": "node scripts/generate-version.js",
        "start": "cross-env NODE_OPTIONS=--max-old-space-size=8192 react-app-rewired start",
        "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 CI=true node scripts/generate-version.js && react-app-rewired build",
        "test": "node scripts/generate-version.js && react-app-rewired test --env=jest-environment-jsdom-sixteen",
        "test-coverage": "node scripts/generate-version.js && react-app-rewired test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage",
        "storybook": "node scripts/generate-version.js && start-storybook -p 9009 -s public",
        "build-storybook": "cross-env NODE_OPTIONS=--max-old-space-size=8192 node scripts/generate-version.js && build-storybook -s public",
        "chromatic": "node scripts/generate-version.js && cross-env NODE_OPTIONS=--max-old-space-size=8192 chromatic  --project-token=rxb9f2dld2q --exit-zero-on-changes --exit-once-uploaded",
        "gen-version": "node scripts/generate-version.js",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "**/*.tsx": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "jest": {
        "globalSetup": "./global-setup.js",
        "moduleNameMapper": {
            "^.+\\.(css|sass|scss|less)$": "identity-obj-proxy",
            "antd/es/(.+)": "antd/lib/$1",
            "@antv/g6/es": "@antv/g6/lib"
        },
        "collectCoverageFrom": [
            "src/**/*.{tsx,ts}",
            "!src/**/*.stories.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0,
                "functions": 0,
                "lines": 0,
                "statements": 16
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11"
        ]
    },
    "optionalDependencies": {
        "fsevents": "*"
    }
}
